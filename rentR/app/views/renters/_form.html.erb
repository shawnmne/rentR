			<% if @renter.errors.any? %>
				<div>
				  <ul>
				    <% @renter.errors.full_messages.each do |e| %>
				      <li><%= e %></li>
				    <% end %>
			  	</ul>
			  </div>
			<% end %>
			<%= bootstrap_form_for @renter do |f| %>
				<div>
					<%= f.text_field :name %>
				</div>
				<div>
					<%= f.text_field :phone %>
				</div>
				<div>
					<%= f.text_field :email %>
				</div>
				<div>
					<%= f.label :rental  %>
			    <%= select("renter", "rental_id", Rental.all.collect {|r| [ (r.address + " " + r.apartment), r.id ] }, {:include_blank => true}) %>
				</div>
				<div>
					<%= f.submit %>
				</div>
			<% end %>